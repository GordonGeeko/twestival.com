<div id="event-sponsors" class="sidebar">
	<div class="title">Sponsors</div>
	<div class="body">
		<!-- Vertical and horizontal aligning of unknown image sizes. http://giveupandusetables.com/ -->
		<table>
			<tbody>
				{{#Event.SponsorRows}}
					<tr>
						{{#.}}
							<td>
								<a href="{{Uri}}">
									<img src="{{ImageUri}}" alt="{{Name}}"/>
								</a>
							</td>
						{{/.}}
					</tr>
				{{/Event.SponsorRows}}
			</tbody>
		</table>
	</div>
</div>