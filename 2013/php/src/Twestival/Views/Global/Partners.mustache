{{> HTMLTag }}
	<head>
		{{> HeadContent }}
		<title>Twestival {{CurrentYear}}: Partners</title>
		
		<link rel="stylesheet" href="css/partners.css" />
	</head>
	<body id="partners">
		{{> Header }}

 		<div class="wrapper">
			<h1>Partners</h1> 
			
			<div class="partner-group" id="partners-sponsors">
				<div class="title">Global Sponsors</div>
				<div class="body">
					<!-- Vertical and horizonal centering of unknown image sizes. http://giveupandusetables.com/ -->
					<!-- TODO: Make manageable -->
					<table class="partners">
						<tbody>
							<tr class="logos">
								<td>
									<a href="https://www.paypal.com/">
										<img src="img/partners/sponsors/content/sponsor-paypal.png" alt="PayPal"/>
									</a>
								</td>
								<td>
									<a href="http://www.project7.com/">
										<img src="img/partners/sponsors/content/sponsor-project7.png" alt="Project 7"/>
									</a>
								</td>
								<td>
									<a href="http://www.paypal.com/">
										<img src="img/partners/sponsors/content/sponsor-stayclassy.png" alt="StayClassy"/>
									</a>
								</td>
							</tr>
							<tr class="captions">
								<td class="caption">
									PayPal is proud to be supporting Twestival organizers and charitable recipients for a second year with some exciting fundraising incentives.
								</td>
								<td class="caption">
									Project 7 is thrilled to partner with Twestival and support US organizing teams by offering special gift packs for local events. Say hello at <a href="mailto:twestival@project7.com">twestival@project7.com</a> for more info.
								</td>
								<td class="caption">
									StayClassy is excited to partner with Twestival to help organizers accept registrations and fundraise for their causes. <a href="http://go.stayclassy.org/landing-twestival">Click here</a> to get started. 
								</td>
							</tr>
						</tbody>
					</table>
					
					<a href="sponsor" id="partners-sponsor">
						Want to sponsor Twestival? We'd love to hear from you! Click here.
					</a>
				</div>
			</div>
			
			<div class="partner-group" id="partners-media">
				<div class="title">Media Partners</div>
				<div class="body">
					<table class="partners">
						<tbody>
							<tr class="logos">
								<td>
									<a href="http://thenextweb.com/">
										<img src="img/partners/media/content/media-thenextweb.png" alt="The Next Web"/>
									</a>
								</td>
								<td>
									&nbsp;
								</td>
								<td>
									&nbsp
								</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
			
			<div class="partner-group" id="partners-media">
				<div class="title">Thank You!</div>
				<div class="body">
					<table class="partners">
						<tbody>
							<tr class="logos">
								<td>
									<a href="http://www.perkinscoie.com/">
										<img src="img/partners/thankyou/content/thankyou-perkinscoie.png" alt="Perkins Coie, LLP"/>
									</a>
								</td>
								<td>
									<a href="http://www.crowdspring.com/">
										<img src="img/partners/thankyou/content/thankyou-crowdspring.png" alt="crowdSPRING"/>
									</a>
								</td>
								<td>
									<a href="http://www.monkeywhale.com/">
										<img src="img/partners/thankyou/content/thankyou-monkeywhale.png" alt="Monkeywhale Productions"/>
									</a>
								</td>
							</tr>
							<tr class="logos">
								<td>
									<a href="https://twitter.com">
										<img src="img/partners/thankyou/content/thankyou-twitter.png" alt="Twitter"/>
									</a>
								</td>
								<td>
									<a href="https://www.mediatemple.net/">
										<img src="img/partners/thankyou/content/thankyou-mediatemple.png" alt="MediaTemple"/>
									</a>
								</td>
								<td>
								</td>
							</tr>

						</tbody>
					</table>
				</div>
			</div>
			
			<div class="clearfix"></div>
			
			{{>Share}}
		</div>
		
		{{> Footer }}
	</body>
</html>